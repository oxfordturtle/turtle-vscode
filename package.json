{
  "name": "turtle-vscode",
  "displayName": "turtle-vscode",
  "description": "Turtle languages support for VSCode",
  "version": "0.0.2",
  "license": "MIT",
  "publisher": "oxfordturtle",
  "repository": {
    "type": "git",
    "url": "https://github.com/oxfordturtle/turtle-vscode"
  },
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "turtleBASIC",
        "aliases": ["Turtle BASIC", "turtleBASIC"],
        "extensions": [".tbas"],
        "configuration": "./basic-configuration.json"
      },
      {
        "id": "turtleC",
        "aliases": ["Turtle C", "turtleC"],
        "extensions": [".tc"],
        "configuration": "./c-configuration.json"
      },
      {
        "id": "turtleJava",
        "aliases": ["Turtle Java", "turtleJava"],
        "extensions": [".tjav"],
        "configuration": "./java-configuration.json"
      },
      {
        "id": "turtlePascal",
        "aliases": ["Turtle Pascal", "turtlePascal"],
        "extensions": [".tpas"],
        "configuration": "./pascal-configuration.json"
      },
      {
        "id": "turtlePython",
        "aliases": ["Turtle Python", "turtlePython"],
        "extensions": [".tpy"],
        "configuration": "./python-configuration.json"
      },
      {
        "id": "turtleTypeScript",
        "aliases": ["Turtle TypeScript", "turtleTypeScript"],
        "extensions": [".tts"],
        "configuration": "./typescript-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "turtleBASIC",
        "scopeName": "source.turtleBASIC",
        "path": "./syntaxes/turtleBASIC.tmLanguage.json"
      },
      {
        "language": "turtleC",
        "scopeName": "source.turtleC",
        "path": "./syntaxes/turtleC.tmLanguage.json"
      },
      {
        "language": "turtleJava",
        "scopeName": "source.turtleJava",
        "path": "./syntaxes/turtleJava.tmLanguage.json"
      },
      {
        "language": "turtlePascal",
        "scopeName": "source.turtlePascal",
        "path": "./syntaxes/turtlePascal.tmLanguage.json"
      },
      {
        "language": "turtlePython",
        "scopeName": "source.turtlePython",
        "path": "./syntaxes/turtlePython.tmLanguage.json"
      },
      {
        "language": "turtleTypeScript",
        "scopeName": "source.turtleTypeScript",
        "path": "./syntaxes/turtleTypeScript.tmLanguage.json"
      }
    ]
  }
}